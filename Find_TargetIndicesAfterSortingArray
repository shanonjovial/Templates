class Solution:
  def targetIndices(self, nums, target):
  
    nums.sort()
    first, total = 0, 0
    
    if len(nums)==0 or target< min(nums) or target > max(nums):
      return list([])
      
    for i in nums:
      if i<target:
        first+=1
      elif i == target:
        total+=1
    return list(range(first, total+first))
